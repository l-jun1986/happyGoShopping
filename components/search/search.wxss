/* components/search/search.wxss */


/* 搜索栏 */

.search {
  width: 750rpx;
  box-sizing: border-box;
  color: #ccc;
  background-color: #ff2b49;
  padding: 30rpx;
}

.search_in {
  background-color: #fff;
  width: 100%;
  height: 50rpx;
  margin: 0 auto;
  border-radius: 8rpx;
  text-align: center;
  font-size: 14px;
  padding-top: 15rpx;
}

.search_in:focus {
  cursor: text;
}

icon {
  vertical-align: middle;
}
